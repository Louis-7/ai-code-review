  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run test",
      "name": "Run npm test",
      "request": "launch",
      "type": "node-terminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/lib/app/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test azure openai",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test.mjs"
    }
  ]
}